{
  "name": "safematch-backend",
  "version": "1.0.0",
  "description": "Backend para aplicacion de citas SafeMatch - Proyecto DAW",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop safematch-api",
    "pm2:restart": "pm2 restart safematch-api",
    "pm2:delete": "pm2 delete safematch-api",
    "pm2:logs": "pm2 logs safematch-api",
    "pm2:monit": "pm2 monit",
    "pm2:prod": "pm2 start ecosystem.config.js --env production",
    "seed": "node scripts/seed.js",
    "test": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "dating",
    "nodejs",
    "express",
    "mongodb",
    "websocket",
    "webrtc"
  ],
  "author": "Estudiantes DAW",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "pm2": "^5.3.0",
    "socket.io": "^4.6.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
